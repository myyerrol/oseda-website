{
  "name": "oseda-website",
  "version": "1.0.0",
  "description": "A project of oseda-website",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build-local": "vuepress build src",
    "build-deploy-pre": "mkdir -p var; rm -rf var/dist;",
    "build-deploy-aft": "cp -rv src/.vuepress/dist/ var/; rm -rf var/dist/res/.git ",
    "build-deploy": "npm run build-deploy-pre; npm run build-local; npm run build-deploy-aft",
    "dev": "vuepress dev src",
    "deploy": "node ./deploy/index.mjs",
    "clean": "vuepress dev src --clean-cache",
    "update": "npx vp-update"
  },
  "devDependencies": {
    "@vuepress/client": "2.0.0-beta.66",
    "colors": "^1.4.0",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.66",
    "vuepress-theme-hope": "2.0.0-beta.233"
  },
  "deploy": "oseda"
}
